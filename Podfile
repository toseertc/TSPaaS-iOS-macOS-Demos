# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'iOSDemo' do
  platform :ios, '10.0'
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
	
  # Pods for iOSDemo
  pod 'IQKeyboardManagerSwift', '~> 6.5.6'
  pod 'SnapKit', '~> 5.0.0'
  pod 'TSRtc_iOS', '~> 2.3.0'
end

target 'macOSDemo' do
  platform :osx, '10.12'	  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for macOSDemo
  pod 'SnapKit', '~> 5.0.0'
  pod 'TSRtc_macOS', '~> 2.3.0'
end
